<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="418.0" prefWidth="314.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.gui.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email:" />
    <TextField fx:id="emailField" />
    <Button fx:id="loginButton" centerShape="false" mnemonicParsing="false" onAction="#login" text="Login" />
    <Button fx:id="cancelButton" centerShape="false" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
</VBox>
